<div class='test'>
    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <%= devise_error_messages! %>

      <div class="row control-group">
          <div class="field col-xs-4 form-group controls">
              <%= f.text_field :name, class: 'form-control', placeholder: 'Nome Completo' %>
          </div>
      </div>

      <div class="row control-group">
          <div class="field col-xs-4 form-group controls">
              <%= f.text_field :cnpj, class: 'form-control', placeholder: 'CNPJ' %>
          </div>
      </div>

      <div class="row control-group">
          <div class="field col-xs-4 form-group controls">
              <%= f.text_field :phone, class: 'form-control', placeholder: 'Telefone'%>
          </div>
      </div>

      <div class="row control-group">
          <div class="field col-xs-4 form-group controls">
              <%= f.text_field :andress, class: 'form-control', placeholder: 'Endereço' %>
          </div>
      </div>

      <div class="row control-group">
          <div class="field col-xs-4 form-group controls">
              <%= f.email_field :email, class: 'form-control', placeholder: 'Email' %>
          </div>
      </div>

      <div class="row control-group">
          <div class="field col-xs-4 form-group controls">
              <%= f.password_field :password, class: 'form-control', placeholder: 'Senha (minimo de 8 caracteres)' %>
          </div>
      </div>


      <div class="row control-group">
          <div class="field col-xs-4 form-group controls">
              <%= f.password_field :password_confirmation, class: 'form-control', placeholder: 'Confirmação da Senha' %>
          </div>
      </div>

      <div class="actions">
        <%= f.submit "Enviar", :class => 'navbar-link btn btn-info bt-lg'%>
      </div>
    <% end %>

    <%= render "devise/shared/links" %>
</div>