<p id="notice"><%= notice %></p>

<h1>Listing Companies</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Cnpj</th>
      <th>Phone</th>
      <th>Andress</th>
      <th>Password</th>
      <th>Access token</th>
      <th>Uid</th>
      <th>Photo url</th>
      <th>Provider</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @companies.each do |company| %>
      <tr>
        <td><%= company.name %></td>
        <td><%= company.email %></td>
        <td><%= company.cnpj %></td>
        <td><%= company.phone %></td>
        <td><%= company.andress %></td>
        <td><%= company.password %></td>
        <td><%= company.access_token %></td>
        <td><%= company.uid %></td>
        <td><%= company.photo_url %></td>
        <td><%= company.provider %></td>
        <td><%= link_to 'Show', company %></td>
        <td><%= link_to 'Edit', edit_company_path(company) %></td>
        <td><%= link_to 'Destroy', company, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Company', new_company_path %>
